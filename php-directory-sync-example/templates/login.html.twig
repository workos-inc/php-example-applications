<html>
    <head>
        <link rel="stylesheet" href="login.css">
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Inter">
    </head>
    <body class="container_login">
        <div class='flex_column'>
            <div class="flex heading_div">
                <img src="workos-logo-with-text.png" alt="workos logo">
            </div>
            
            <h2>PHP Directory Sync Example App</h2>
            <hr style="width:100%">
                <a href="/webhooks" class="button button-outline width-95">Test Webhooks</a>
            <hr style="width:100%">
            {% for i in directories %}
                <a class="button width-95" href="/directory?id={{ i.raw['id'] }}">{{ i.raw['name'] }}</a>
            {% endfor %}
        </div>
    </body>
</html>
